{
    "title": "vim binds",
    "rules": [
        {
            "description": "bind H",
            "manipulators": [
                {
                    "from": {
                        "key_code": "h",
                        "modifiers": {
                            "mandatory": [
                                "left_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "osascript ~/.config/nvim/plugged/vim-iterm2-navigator/applescript/forward_to_vim_or_term h"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
